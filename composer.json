{
  "name": "blog/app",
  "type": "project",
  "description": "The amazing blog management side project platform .",
  "keywords": [
    "blog",
    "management"
  ],
  "require": {
    "php": "^8.1.6",
    "doctrine/orm": "^2.11.0",
    "doctrine/dbal": "^3.2",
    "symfony/yaml": "^5.4",
    "symfony/cache": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "10.3.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}